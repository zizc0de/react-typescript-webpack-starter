{
  "name": "react-typescript-webpack-starter",
  "version": "0.1.0",
  "description": "React Typescript Webpack Starter",
  "main": "public/index.html",
  "author": "zizcode",
  "license": "MIT",
  "scripts": {
    "build:client": "webpack --config webpack.prod.ts",
    "build:server": "webpack --config webpack.config.server.ts",
    "client": "webpack-dev-server --config webpack.dev.ts",
    "dev": "concurrently \"nodemon\" \"npm run client\"",
    "build": "concurrently --kill-others-on-fail \"npm run build:server\" \"npm run build:client\"",
    "start": "node -r dotenv/config ./dist/server/server.js",
    "lint": "eslint --ext .ts --ext .tsx src",
    "lint:fix": "eslint --ext .ts --ext .tsx src --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "jest",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "http-status-codes": "^2.1.4",
    "lightship": "^6.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.1",
    "typescript": "^4.4.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.13",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.0.3",
    "@types/webpack-node-externals": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "autoprefixer": "^10.3.3",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.2.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "path": "^0.12.7",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "sass-loader": "^12.1.0",
    "standard-version": "^9.3.1",
    "style-loader": "^3.2.1",
    "tailwindcss": "^2.2.9",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "uninstall": "0.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
